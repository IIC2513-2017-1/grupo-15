<div class="project-index">
  <h1>Some of our projects</h1>
  <% @projects.first(10).each do |project| %>
    <%= render 'projects/project', project: project %>
  <% end %>
  <br>
  <% if logged_in? %>
    <%= link_to 'Create a project', new_project_path, class:"button btn-red" %>
  <% end %>
</div>
